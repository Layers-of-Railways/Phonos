{
  "schemaVersion": 1,
  "id": "phonos",
  "version": "${version}",

  "name": "Phonos",
  "description": "Fill your world with music!",
  "authors": [
    "FoundationGames",
    "techno-sam"
  ],
  "contact": {
    "sources": "https://github.com/Layers-of-Railways/Phonos",
    "issues": "https://github.com/Layers-of-Railways/Phonos/issues"
  },

  "license": "MIT",
  "icon": "assets/phonos/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.foundationgames.phonos.Phonos"
    ],
    "client": [
      "io.github.foundationgames.phonos.PhonosClient"
    ],
    "modmenu": [
      "io.github.foundationgames.phonos.util.compat.PhonosModMenu"
    ],
    "voicechat": [
      "io.github.foundationgames.phonos.util.compat.PhonosVoicechatPlugin"
    ]
  },
  "mixins": [
    "phonos.mixins.json"
  ],
  "accessWidener": "phonos.accesswidener",
  "depends": {
    "fabricloader": ">=0.7.4",
    "fabric": "*",
    "minecraft": "1.20.x"
  },
  "suggests": {
    "voicechat": ">=2.4.16"
  }
}
